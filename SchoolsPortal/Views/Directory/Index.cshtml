<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
    <script src="~/Scripts/JavaScript.js"></script>
    <link rel="stylesheet" href="styles.css">
</head>
<body>
    <table id="tables">
        <tr>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
            <th>Suffix</th>
            <th>Sex</th>
            <th>Grade</th>
            <th>Card ID</th>
        </tr>
        @foreach (dynamic p in (ViewBag.directory))
        {
        <tr>
            <td>@p.getname().getfirstname()</td>
            <td>@p.getname().getmiddlename()</td>
            <td>@p.getname().getlastname()</td>
            <td>@p.getname().getsuffix()</td>
            <td>@p.getname().getsex()</td>
            <td>@p.getgrade()</td>
            <td>@p.getcardid()</td>
        </tr>
        }
    </table>
</body>
</html>